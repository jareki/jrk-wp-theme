{
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "1500px",
			"wideSize": "1920px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Scada",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/scada_normal_400.ttf"
							]
						},
						{
							"fontFamily": "Scada",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/scada_italic_400.ttf"
							]
						}
					],
					"fontFamily": "Scada",
					"slug": "scada"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Rasa",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/rasa_normal_300.ttf"
							]
						},
						{
							"fontFamily": "Rasa",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/rasa_normal_600.ttf"
							]
						},
						{
							"fontFamily": "Rasa",
							"fontStyle": "italic",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/rasa_italic_300.ttf"
							]
						},
						{
							"fontFamily": "Rasa",
							"fontStyle": "italic",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/rasa_italic_600.ttf"
							]
						}
					],
					"fontFamily": "Rasa",
					"slug": "rasa"
				}
			],
			"fontSizes": [
				{
					"fluid": {
						"max": "0.84rem",
						"min": "0.79rem"
					},
					"name": "Small",
					"size": "0.8rem",
					"slug": "sm-r"
				},
				{
					"fluid": {
						"max": "1.13rem",
						"min": "0.97rem"
					},
					"name": "Base",
					"size": "1rem",
					"slug": "bs-r"
				},
				{
					"fluid": {
						"max": "1.5rem",
						"min": "1.18rem"
					},
					"name": "Medium",
					"size": "1.25rem",
					"slug": "md-r"
				},
				{
					"fluid": {
						"max": "2rem",
						"min": "1.45rem"
					},
					"name": "Large",
					"size": "1.56rem",
					"slug": "lg-r"
				},
				{
					"fluid": {
						"max": "2.66rem",
						"min": "1.77rem"
					},
					"name": "XL",
					"size": "1.95rem",
					"slug": "xl-r"
				},
				{
					"fluid": {
						"max": "3.55rem",
						"min": "2.15rem"
					},
					"name": "XXL",
					"size": "2.44rem",
					"slug": "xxl-r"
				},
				{
					"fluid": {
						"max": "4.73rem",
						"min": "2.61rem"
					},
					"name": "XXXL",
					"size": "3.05rem",
					"slug": "xxxl-r"
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/site-title": {
				"typography": {
					"textTransform": "uppercase"
				}
			}
		},
		"color": {
			"text": "var(--wp--preset--color--black)"
		},
		"elements": {
			"button": {
				"color": {
					"gradient": "linear-gradient(135deg,rgb(31,31,31) 0%,rgb(45,73,117) 100%)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--scada)",
					"letterSpacing": "1px",
					"lineHeight": "1",
					"textTransform": "uppercase"
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--black)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--scada)",
					"fontSize": "var(--wp--preset--font-size--sm-r)",
					"fontStyle": "normal",
					"fontWeight": "600",
					"letterSpacing": "0.03rem",
					"textTransform": "uppercase"
				}
			},
			"h1": {
				"typography": {
					"fontSize": "clamp(3.05rem, 1.77vi + 2.61rem, 4.73rem)",
					"lineHeight": "1.1"
				}
			},
			"h2": {
				"typography": {
					"fontSize": "clamp(2.44rem, 1.17vi + 2.15rem, 3.55rem)",
					"lineHeight": "1.1"
				}
			},
			"h3": {
				"typography": {
					"fontSize": "clamp(1.95rem, 0.75vi + 1.77rem, 2.66rem)",
					"lineHeight": "1.1"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "clamp(1.56rem, 0.46vi + 1.45rem, 2rem)",
					"lineHeight": "1.1"
				}
			},
			"h5": {
				"typography": {
					"fontSize": "clamp(1.25rem, 0.26vi + 1.18rem, 1.5rem)",
					"lineHeight": "1.1"
				}
			},
			"h6": {
				"typography": {
					"fontSize": "clamp(1rem, 0.13vi + 0.97rem, 1.13rem)",
					"lineHeight": "1.1"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--scada)",
					"fontStyle": "normal",
					"fontWeight": "800",
					"letterSpacing": "2px",
					"textTransform": "uppercase"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--black)"
					},
					"typography": {
						"textDecoration": "underline"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--black)"
				},
				"typography": {
					"textDecoration": "none"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--rasa)",
			"fontStyle": "normal",
			"fontWeight": "700"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2,
	"$schema": "https://schemas.wp.org/wp/6.5/theme.json"
}